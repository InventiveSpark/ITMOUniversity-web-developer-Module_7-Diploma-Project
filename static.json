{
  "headers": {
    "/**": {
      "Access-Control-Allow-Origin": "*"
    },
    "/images/**": {
      "Cache-Control": "public, max-age=31536000"
    },
    "/css/**": {
      "Cache-Control": "public, max-age=31536000"
    }
  },
  "routes": {
    "/images/*": "images/",
    "/css/styles.css": "css/styles.css",
    "/": "index.html",
    "/*.html": "/"
  }
}